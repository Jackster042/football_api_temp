    "@vitejs/plugin-react": "^4.3.0",
    "dotenv": "^16.4.0",
    "drizzle-kit": "^0.30.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.0",
    "tsx": "^4.19.0",
